def sortAndFindMedian(numbers):
  sort(numbers)
  n = len(numbers)
  if n%2 == 0:
    return ((numbers[(n // 2) - 1] + numbers[n // 2] ) // 2)
  else:
    return numbers[n // 2]

def sort(numbers):
  numbers = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
  result = sort_and_find_median(numbers)
  numbers.sort()
  print("Sorted Numbers:", numbers)
  print("Median:", result)
